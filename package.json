{
  "name": "@rudderstack/rudder-sdk-node",
  "version": "2.0.0",
  "description": "Rudder Node SDK",
  "license": "",
  "repository": "rudderlabs/rudder-sdk-node",
  "author": {
    "name": ""
  },
  "engines": {
    "node": ">=4"
  },
  "size-limit": [
    {
      "limit": "25 KB",
      "path": "index.js"
    }
  ],
  "scripts": {
    "size": "size-limit",
    "build": "babel index.js --out-dir lib && cp package.json lib/ && cp readme.md lib/ && cp History.md lib/ && cp LICENSE.md lib/ && cp index.d.ts lib/",
    "test": "AVA_MODE_ON=true nyc --reporter=lcov --reporter=html --reporter=text ava --serial --verbose  > coverage.lcov",
    "test:ci": "AVA_MODE_ON=true nyc --reporter=lcov --reporter=html --reporter=text ava --serial --verbose  > coverage.lcov",
    "changelog": "auto-changelog -p -t keepachangelog -u true -l false --sort-commits date-desc ",
    "tsc": "tsc",
    "generateHistory": "auto-changelog -p -r origin --tag-prefix v --tag-pattern 'v(1.0.(0|[1-9]d*)|1.1.[0-4]|2.(0|[1-9]d*).(0|[1-9]d*))' -l false -o HISTORY.md -t keepachangelog",
    "check:lint": "eslint index.js",
    "check:lint:ci": "eslint index.js -f json -o reports/eslint.json || exit 0",
    "check:circular": "madge --circular --extensions js index.js || exit 0",
    "check:duplicates": "jscpd index.js --threshold 5",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release": "npx standard-version",
    "release:github": "npx conventional-github-releaser -p angular"
  },
  "main": "index.js",
  "keywords": [
    "analytics"
  ],
  "dependencies": {
    "@segment/loosely-validate-event": "^2.0.0",
    "axios": "0.26.0",
    "axios-retry": "^3.2.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isstring": "^4.0.1",
    "md5": "^2.3.0",
    "ms": "^2.1.3",
    "remove-trailing-slash": "^0.1.1",
    "serialize-javascript": "^6.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/config-conventional": "17.3.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "7.4.2",
    "auto-changelog": "^2.4.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.17.1",
    "codecov": "^3.8.1",
    "commander": "^9.0.0",
    "commitizen": "4.2.5",
    "commitlint": "17.3.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "delay": "^5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-unicorn": "45.0.0",
    "express": "^4.17.3",
    "husky": "8.0.2",
    "jscpd": "3.5.1",
    "lint-staged": "13.0.4",
    "madge": "5.0.1",
    "np": "^7.5.0",
    "nyc": "^15.1.0",
    "pify": "^5.0.0",
    "prettier": "2.8.0",
    "sinon": "^7.3.2",
    "size-limit": "^7.0.8",
    "snyk": "^1.171.1",
    "standard": "^12.0.1",
    "standard-version": "9.5.0",
    "typescript": "^4.6.2"
  },
  "overrides": {
    "kind-of": "^6.0.3",
    "lodash": "^4.17.19",
    "set-value": "^2.0.1",
    "yargs-parser": "^13.1.2",
    "trim-newlines": "^3.0.1",
    "minimist": "^1.2.6",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^3.0.1",
    "dot-prop": "5.3.0",
    "semver": "7.3.8",
    "semver-regex": "3.1.4",
    "json5": "2.2.2"
  },
  "resolutions": {
    "kind-of": "^6.0.3",
    "lodash": "^4.17.19",
    "set-value": "^2.0.1",
    "yargs-parser": "^13.1.2",
    "trim-newlines": "^3.0.1",
    "minimist": "^1.2.6",
    "glob-parent": "^5.1.2",
    "ansi-regex": "^3.0.1",
    "dot-prop": "5.3.0",
    "semver": "7.3.8",
    "semver-regex": "3.1.4",
    "json5": "2.2.2"
  },
  "optionalDependencies": {
    "bull": "^4.10.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{json,js,md,ts}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraMode": false,
      "jiraOptional": true,
      "skipScope": true,
      "defaultType": "feat"
    }
  }
}
